<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="web_pqr_category_view_tree" model="ir.ui.view">
        <field name="name">web.pqr.category.view.tree</field>
        <field name="model">web.pqr.category</field>
        <field name="arch" type="xml">
            <tree string="Categorías" sample="1">
                <field name="name"/>
            </tree>
        </field>
    </record>
 


    <record  id="web_pqr_category_view_search"  model="ir.ui.view">
        <field name="name">web.pqr.category.view.search</field>
        <field name="model">web.pqr.category</field>
        <field name="arch" type="xml">
            <search string="Busqueda de Categorías">
                <group expand="0" string="Agrupar por">
                </group>
            </search>
        </field>
        </record>

</odoo>